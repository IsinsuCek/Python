def find_sum(list,num):
    count = 0 

    for i in range(len(list)):
        limit = i
        count = list[i] + count

        if count == num:
           for j in range(limit + 1):
            print(list[j], end=' ')
            count = 0
            limit = 0


my_list = [1,2,3,3,4,5,6,7,8,9]
my_num = int(input("LÃ¼tfen bir sayÄ± giriniz: "))
find_sum(my_list,my_num)